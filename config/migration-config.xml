<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- List of applications installed on the current machine with path to the executable.
			If @executable starts with "Class:", the FileConverter will try to run the main method of the given class. -->
	<!-- Parameters given on this level will be used if no other parameters are given with the specific migration for a PUID/mimetype/extension.
			The string "{[arg1]}" will be replaced with the source filename, "{[arg2]}" will be replaced with the target filename. -->
	<application id="1"	name="OpenOffice"		executable="Class:ch.docuteam.converter.OOConverter"
																parameter="{[arg1]}#{[arg2]}" />
	<application id="2"	name="FFmpeg"			executable="ffmpeg" />
	<application id="3"	name="Ghostscript"		executable="gs"
																parameter="-dPDFA=2#-dBATCH#-dNOPAUSE#-dNOSAFER#-dPDFACompatibilityPolicy=1#-sColorConversionStrategy=RGB#-sDEVICE=pdfwrite#-sOutputFile={[arg2]}#%ACTIONS_INSTALL_DIR%\PDFA_def.ps#{[arg1]}" />
	<application id="4"	name="ImageMagick"		executable="convert"
																parameter="-compress#none#{[arg1]}#{[arg2]}" />
	<!-- SIARD does only work on windows platforms -->
	<application id="5"	name="SIARD Suite"		executable="script\convmdb.cmd"
																parameter="{[arg1]}#{[arg2]}" />
	<!-- <application id="6"	name="PDF-Tools"		executable="Class:ch.docuteam.converter.PDFToolsConverter"
																parameter="{[arg1]}#{[arg2]}" />
	<application id="10" name="PDFToolsConverterWSClient"       executable="Class:ch.docuteam.converter.PDFToolsConverterWSClient"
						 										parameter="\-\-path-to-input={[arg1]}#\-\-path-to-output={[arg2]}#\-\-spPropertiesFile=config/pdfToolsConverterWS.properties#\-\-j=ConverterOptions#\-\-byName=false#\-\-ax=path/to/xmpdata.xml#\-\-param-name=param-xyz#\-\-param-file=path/to/paramFile"/> -->

